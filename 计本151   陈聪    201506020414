#include<bits/stdc++.h>#include<string.h>
using namespace std;#include<stdlib.h>#include<time.h> #define ARR 10000int CreatData(long int arr[],int n,int min,int max)  //用来产生10000个随机数，后面把随机数转换成字符 { int i,j,flag; srand(time(NULL)); if((max-min+1)>n)return 0; for(i=0;i<n;i++) {  do{   arr[i]=(max-min+1)*rand()/(RAND_MAX+1)+min;   flag=0;   for(j=0;j<i;j++)   {    if(arr[i]==arr[j])    flag=0;    }    }while(flag); } return 1; }void cal_next(char *source, int *next, int len){    next[0] = -1;                              //next[0]初始化为-1，-1表示不存在相同的最大前缀和最大后缀    int k = -1;                                //k初始化为-1    for (int q = 1; q <= len-1; q++)    {        while (k > -1 && source[k + 1] != source[q])//如果下一个不同，那么k就变成next[k]，注意next[k]是小于k的，无论k取任何值。        {            k = next[k];                           //往前回溯        }        if (source[k + 1] == source[q])//如果相同，k++        {            k = k + 1;        }        next[q] = k;//这个是把算的k的值（就是相同的最大前缀和最大后缀长）赋给next[q]    }}int KMP(char *source, int slen, char *target, int plen){    int *next = new int[plen];    cal_next(target, next, plen);                //计算next数组    int k = -1;    for (int i = 0; i < slen; i++)    {        while (k >-1&& target[k + 1] != source[i])//target和source不匹配，且k>-1（表示target和source有部分匹配）            k = next[k];            )    {        // cout<<x<<endl;        int c=x;                //用来定位从那个数开始有相同的序列         int d=0;    for(int y=0;y<b;y++) {  //cout<<x<<endl;  if(source[c]==target[y]) //一一的进行比较   {      d++;      c++;      continue;  }  else break; }  if(d==b)                    //找到有一样长度的序列，打印出该位置的起始位置  {     cout<<"1"<<endl;     cout<<"在"<<c-b<<"这个位置有序列"<<endl; } else                         //没有找到这个长度的序列，打印出-1  {     if(x<ARR-1)     continue;     else cout<<"-1"<<"表示没有序列与之相等"<<endl; }  finish1=clock();    totaltime1=(double)(finish1-start1)/CLOCKS_PER_SEC;    cout<<"\n双重for循环的运行时间为"<<totaltime1<<"秒！"<<endl;    }    clock_t start,finish;    double totaltime;    start=clock();    int c= KMP(source,ARR, target,b);    if(c==-1)    {     cout<<"-1"<<endl;     cout<<"字符序列没有与之相匹配的序列"<<endl;  } else    cout<<"在原字符串的第"<<c<<"个位置有匹配的字符串"<<endl;    finish=clock();    totaltime=(double)(finish-start)/CLOCKS_PER_SEC;    cout<<"\nkMP算法的运行时间为"<<totaltime<<"秒！"<<endl;           return 0;}











#include<bits/stdc++.h> #include<time.h>  #include<stdlib.h> 
using namespace std;void quickSort(int src[], int left, int right) { if (left<right)  {  int key = src[left];  int i = left;  int j = right;  while (i < j)  {   while (i < j && src[j] > key)   {    j--;   }   if (i < j)    {    src[i] = src[j];    i++;   }   while (i < j && src[i] < key)    {    i++;   }   if (i < j)   {    src[j] = src[i];    j--;   }  }  src[i] = key;  quickSort(src,left, i - 1);  quickSort(src, i + 1, right); }}//非递归 int BinarySearch(int array[], int aSize, int key){    if ( array == NULL || aSize == 0 )        return -1;    int low = 0;    int high = aSize - 1;    int mid = 0;    while ( low <= high )            //一直循环，直到算出为止     {        mid = (low + high )/2;        if ( array[mid] < key)            low = mid + 1;        else if ( array[mid] > key )               high = mid - 1;        else            return mid;    }    return -1;}    //递归 int search(int arr[],int n,int left,int right){ int mid=(left+right)/2; if(n<arr[left] || n>arr[right] || arr[left]>arr[right])//值不是有序的  {     return -1; } if(arr[mid]<n)          //查找的值在右边  {  return search(arr,n,mid+1,right); } else if(arr[mid]>n)     //查找的值在左边  {  return search(arr,n,left,mid-1); } else  return mid;}int main(){ int a[100000]; srand((unsigned)time(NULL)); for(int i=0;i<100000;i++)     a[i]=rand()%99999+i; quickSort(a,0,99999); for(int i=0;i<10000;i++)     cout<<a[i]<<" ";  cout<<endl; int X=rand(); cout<<"待查找的数据为"<<X<<endl; clock_t start,finish;    long double totaltime1s; start=clock(); for(int i=0;i<10000000;i++) {     BinarySearch(a, 100000, X);    } finish=clock(); totaltime1s=(long double)(finish-start)/CLOCKS_PER_SEC; cout<<"用非递归二分查找程序的运行时间为"<<totaltime1s<<"秒"<<endl; int b= BinarySearch(a, 100000, X);    if(b==-1)     cout<<"没有查找到该数字"<<endl;  else      cout<<"要查找的数字在"<<b<<"处查找到了" <<endl; clock_t start1,finish1;    long double totaltime2s; start1=clock(); for(int i=0;i<10000000;i++) {     search(a,X,0,99999);    } finish1=clock(); totaltime2s=(long double)(finish1-start1)/CLOCKS_PER_SEC; cout<<"用递归二分查找程序的运行时间为"<<totaltime2s<<"秒"<<endl; int c=search(a,X,0,99999); if(c==-1)     cout<<"没有查找到该数字"<<endl;  else      cout<<"要查找的数字在"<<c<<"处查找到了" <<endl;  }
           
           
           
           
           
  
  
  
  
  
  #include <bits/stdc++.h>    using namespace std;    #include <stdio.h>  #include<time.h>  #include<stdlib.h>          typedef struct BiTNode    {        int value;        struct BiTNode *lchild,*rchild;    }*BiTree;        bool LT(int a,int b)  //LessThan小于    {        if(a<b)            return true;        else            return false;    }    /*  在根指针root所指向的二叉排序树中递归地查找其关键字等于data的数据元素，若查找成功，则指针p指向该数据元素结点，并返回true，  否则指针p指向查找路径上访问的最后一个结点并返回false指针，指针f指向root的双亲，其初始调用值NULL  */    bool SearchBST(BiTree root,int data,BiTree f,BiTree &p)    {        if(!root)        {            p=f;            return false;        }        else if(data==root->value)        {            p=root;            return true;        }        else if(data<root->value)            return SearchBST(root->lchild,data,root,p);        else if(data>root->value)            return SearchBST(root->rchild,data,root,p);    }        //当二叉排序树root中不存在关键字等于data的数据元素时，插入data，此函数调用频率高，所以设置成inline  inline void InsertBST(BiTree &root,int data)     //root为传引用指针    {          BiTree p,s;        if(!SearchBST(root,data,NULL,p))    //查找不成功        {            s=(struct BiTNode *)malloc(sizeof(BiTNode));            s->value=data;            s->lchild=s->rchild=NULL;           if(p==NULL)    //二叉排序树为空的时候，被插入结点*s为新的根结点                root=s;            else if(LT(data,p->value))           //被插结点*s为左孩子                p->lchild=s;            else           //被插结点*s为右孩子               p->rchild=s;        }        return ;    }    void PreOrderTraverse(BiTree root)    //先序遍历    {        if(root)        {      cout<<root->value<<" ";           PreOrderTraverse(root->lchild);            PreOrderTraverse(root->rchild);        }    }    void InOrderTraverse(BiTree root)    //中序遍历    {        if(root)        {            InOrderTraverse(root->lchild);            cout<<root->value<<" ";              InOrderTraverse(root->rchild);        }    }    void PostOrderTraverse(BiTree root)    //后序遍历    {        if(root)        {            PostOrderTraverse(root->lchild);            PostOrderTraverse(root->rchild);            cout<<root->value<<" ";         }    }    void DeleteBST(BiTree root)  //释放二叉树占用的内存空间  {        if(root)        {            DeleteBST(root->lchild);    //释放左子树            DeleteBST(root->rchild);    //释放右子树            free(root);        //释放根结点        }    }    int main(void)    {        int i,n;   int a[100000]; srand((unsigned)time(NULL)); for(int i=0;i<100000;i++)     a[i]=rand()%99999+i;      BiTree root;        root=NULL;        for(i=0;i<100000;i++)        {      cout<<a[i];          InsertBST(root,a[i]);        }        clock_t start,finish;    long double totaltimes; start=clock();    PreOrderTraverse(root);     cout<<endl;  finish=clock(); totaltimes=(long double)(finish-start)/CLOCKS_PER_SEC; cout<<"二叉搜索树先序的运算时间为"<<totaltimes<<"秒"<<endl;    clock_t start1,finish1;    long double totaltime1s; start1=clock();    InOrderTraverse(root);  cout<<endl;    finish1=clock(); totaltime1s=(long double)(finish1-start1)/CLOCKS_PER_SEC; cout<<"二叉搜索树中序的运算时间为"<<totaltime1s<<"秒"<<endl; clock_t start2,finish2;    long double totaltime2s; start2=clock();          PostOrderTraverse(root); cout<<endl;   finish2=clock(); totaltime2s=(long double)(finish2-start2)/CLOCKS_PER_SEC; cout<<"二叉搜索树后序的运算时间为"<<totaltime2s<<"秒"<<endl;    cout<<"二叉搜索树先序的运算时间为"<<totaltimes<<"秒"<<endl;  cout<<"二叉搜索树中序的运算时间为"<<totaltime1s<<"秒"<<endl;       DeleteBST(root);            return 0;    }  

                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                           
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    #include<bits/stdc++.h>
using namespace std;#include <stdio.h>  #include <stdlib.h>  #define MAXVEX 1001  #define MAXEDGE 100000    #define INFINITY 0x3fffffff  //构造边集  typedef struct Edge  {      int cost;   //花费      int lenth;  //长度  } Edge;    Edge edges[MAXVEX][MAXVEX];      void CreateMGraph(int numVertexes, int numEdges)  {      int i, j;      int a, b;      int len, cost;      for (i = 1; i <= numVertexes; i++)   {          for (j = 1; j <= numVertexes; j++)         {             edges[i][j].lenth = INFINITY;             edges[i][j].cost = INFINITY;         }     }        for (i = 0; i < numEdges; i++)      {          scanf("%d%d%d%d", &a, &b, &len, &cost);          if (edges[a][b].lenth > len)          {              edges[a][b].lenth = len;              edges[a][b].cost = cost;              edges[b][a].lenth = len;              edges[b][a].cost = cost;          }          else if (edges[a][b].lenth == len)          {              if (edges[a][b].cost > cost)              {                  edges[a][b].cost = cost;                  edges[b][a].cost = cost;              }          }      }  }  typedef int ShortPathTable[MAXVEX];  typedef int Cost[MAXVEX];   //Dijkstra算法  void ShortestPath_Dijkstra(int numVertexes, int numEdges, int v0, int vm, int *len, int *cost)  {      int v, w, k, min;      int min_cost;      ShortPathTable D;      int final[MAXVEX];  //标记是否求得顶点v0-vm的路径      Cost C;       *len = INFINITY;      *cost = INFINITY;        for (v = 1; v <= numVertexes; v++)      {          final[v] = 0;    D[v] = edges[v0][v].lenth;            C[v] = edges[v0][v].cost;       }      final[v0] = 1;     //开始主循环，每次求得v0到某v顶点的最短路径      for (v = 2; v <= numVertexes; v++)      {          min = INFINITY;           //当前所知离v0顶点的最近距离          min_cost = 0;          for (w = 1; w <= numVertexes; w++)   //寻找离v0最近的顶点        {              if (!final[w] && D[w] < min)              {                  k = w;                  min = D[w];  //w顶点离v0更近                  min_cost = C[w];              }      }      final[k] = 1;      if (k == vm)      {          *len = D[k];          *cost = C[k];          // break;      }      for (w = 1; w <= numVertexes; w++)      {          if (!final[w] && (min+edges[k][w].lenth) < D[w])  //如果经过v顶点的路径比现在这条路径的长度短的话          {              //说明找到了更短的路径，修改D[w],C[w]              D[w] = min + edges[k][w].lenth;              C[w] = min_cost + edges[k][w].cost;          }          else if (!final[w] && (min+edges[k][w].lenth) == D[w])  //如果经过v顶点的路径与现在这条路径相等          {              if (min_cost + edges[k][w].cost < C[w])    //取花费较少的值赋值              {                  C[w] = min_cost + edges[k][w].cost;              }          }      }  }  int main()  {      int numVertexes = 3;      int numEdges = 2;      int start;      int end;      int len;      int cost;      while (scanf("%d%d", &numVertexes, &numEdges) && numVertexes != 0 && numVertexes != 0)      {          CreateMGraph(numVertexes, numEdges);          scanf("%d%d", &start, &end);          ShortestPath_Dijkstra(numVertexes, numEdges, start, end, &len, &cost);          printf("%d %d\n", len, cost);      }      return 0;  }                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                          
